<?xml version="1.0" encoding="UTF-8"?>

<infinispan xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="urn:infinispan:config:5.1">
   <global>
      <transport clusterName="x" distributedSyncTimeout="600000">
         <properties>
            <property name="configurationFile" value="jgroups/jgroups-udp.xml"/>
         </properties>
      </transport>
   </global>

   <default>
      <unsafe unreliableReturnValues="true" />
      <locking isolationLevel="REPEATABLE_READ" lockAcquisitionTimeout="3000" concurrencyLevel="1000" useLockStriping="false"/>
      <indexing enabled="false" />
      <invocationBatching enabled="false" /> 
      <eviction strategy="NONE"/>

      <clustering mode="d">
         <sync replTimeout="60000"/>
         <l1 enabled="false" />
         <hash numOwners="2" rehashRpcTimeout="600000" rehashWait="600000" numVirtualNodes="512" />
         <stateTransfer fetchInMemoryState="true" timeout="600000"/>
      </clustering>
   </default>

   <namedCache name="x" />
</infinispan>
